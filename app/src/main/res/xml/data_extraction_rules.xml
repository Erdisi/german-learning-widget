<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Include user preferences and customizations for backup -->
        <include domain="sharedpref" path="user_preferences"/>
        <include domain="sharedpref" path="widget_customizations"/>
        <include domain="sharedpref" path="app_settings"/>
        
        <!-- Exclude cache and temporary data -->
        <exclude domain="cache"/>
        <exclude domain="no_backup"/>
    </cloud-backup>
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules>